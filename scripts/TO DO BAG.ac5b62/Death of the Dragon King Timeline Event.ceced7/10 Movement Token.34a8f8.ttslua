isKdmEffectsObject = true
effects = {
    move = 1
}

function getEffects()
    rot = self.getRotation()
    if rot[3] > 345 or rot[3] < 15 then
        effects.movement = -1
    else
        effects.movement = 10
    end
	local qty = self.getQuantity()
	if qty >= 2 then
		effects.move = effects.move * qty
	end
    return effects
end
